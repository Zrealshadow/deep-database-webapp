      "time_column": "CreationDate",
      "foreign_keys": [
        {"column": "UserId", "references": "users.Id"},
        {"column": "PostId", "references": "posts.Id"}
      ],
      "sample_columns": ["Id", "PostId", "Score", "Text", "CreationDate", "UserId"]
    },
    {
      "name": "votes",
      "description": "User votes (upvotes, downvotes, favorites) on posts",
      "num_rows": 8000000,
      "num_columns": 5,
      "primary_key": "Id",
      "time_column": "CreationDate",
      "foreign_keys": [
        {"column": "PostId", "references": "posts.Id"},
        {"column": "UserId", "references": "users.Id"}
      ],
      "sample_columns": ["Id", "PostId", "VoteTypeId", "CreationDate", "UserId"]
    },
    {
      "name": "badges",
