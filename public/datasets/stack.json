{
  "id": "stack",
  "name": "Stack Overflow",
  "category": "Social Network",
  "description": "Stack Overflow forum data with questions, answers, comments, votes, badges, and tag relationships. This dataset captures the rich interaction patterns in one of the largest programming Q&A communities.",
  "source": "RelBench (Stanford)",
  "download_url": "https://relbench.stanford.edu",
