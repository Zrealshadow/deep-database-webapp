      "num_rows": 1500000,
      "num_columns": 4,
      "primary_key": "Id",
      "time_column": "Date",
      "foreign_keys": [
        {"column": "UserId", "references": "users.Id"}
      ],
      "sample_columns": ["Id", "UserId", "Name", "Date"]
    },
    {
      "name": "postLinks",
      "description": "Links between related posts (duplicates, related questions)",
      "num_rows": 300000,
      "num_columns": 4,
      "primary_key": "Id",
      "time_column": "CreationDate",
      "foreign_keys": [
        {"column": "PostId", "references": "posts.Id"},
        {"column": "RelatedPostId", "references": "posts.Id"}
      ],
      "sample_columns": ["Id", "PostId", "RelatedPostId", "LinkTypeId", "CreationDate"]
    },
    {
      "name": "postHistory",
      "description": "Edit history and modifications to posts",
      "num_rows": 200000,
      "num_columns": 7,
