      "description": "User profiles and account information",
      "num_rows": 5000000,
      "num_columns": 8,
      "primary_key": "Id",
      "time_column": "CreationDate",
      "foreign_keys": [],
      "sample_columns": ["Id", "Reputation", "CreationDate", "DisplayName", "Views", "UpVotes", "DownVotes"]
    },
    {
      "name": "posts",
      "description": "Questions and answers with self-referential relationships",
      "num_rows": 20000000,
      "num_columns": 15,
      "primary_key": "Id",
      "time_column": "CreationDate",
      "foreign_keys": [
