      "foreign_keys": [
        {"column": "PostId", "references": "posts.Id"},
        {"column": "UserId", "references": "users.Id"}
      ],
      "sample_columns": ["Id", "PostId", "PostHistoryTypeId", "RevisionGUID", "CreationDate", "UserId", "Text"]
    },
    {
      "name": "tags",
      "description": "Tag reference table (augmented)",
      "num_rows": 50000,
      "num_columns": 2,
      "primary_key": "Id",
      "time_column": null,
      "foreign_keys": [],
